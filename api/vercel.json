{
    "installCommand":"npm install --legacy-peer-deps",
    "version": 2,
    "builds" : [
        {
            "src": "server.js",
            "use": "@vercel/json"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {"src": "/(.*)", "dest": "/"}
    ]
}